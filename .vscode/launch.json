    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package with File storage",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/kmfddm/main.go",
            "args": [ "-api", "kmfddm", "-enqueue", "http://[::1]:9000/v1/enqueue/", "-enqueue-key", "nanomdm", "-debug"]
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/kmfddm/main.go",
            "args": [ "-api", "kmfddm", "-enqueue", "http://[::1]:9000/v1/enqueue/", "-enqueue-key", "nanomdm", "-debug", "-storage", "dynamo"]
        },
        {
            "name": "Test Dynamo Storage",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}",
            "showLog": false,
            "buildFlags": "-v --tags=integration ./storage/dynamo/ -args -dsn"
        }
    ]
}